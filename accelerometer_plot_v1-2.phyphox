<phyphox version="1.7" locale="en">
    <title>Acceleration</title>
    <category>Arduino Nano 33 BLE Sense</category>
    <description>Plots acceleration measured by the Arduino Nano 33 BLE Sense's LSM9DS1 IMU</description>
    <link label="Find more">www.physicsreimagined.com</link>
<icon format="base64"></icon> 
	<color>009297</color> 
    <translations>
        <translation locale="fr">
            <title>Accélération</title>
            <category>Arduino Nano 33 BLE Sense</category>
            <description>Trace l'accélération mesurée par les capteurs de la carte Arduino.</description>
            <link label="Plus d'infos">http://www.vulgarisation.fr</link>
            <string original="Accelerometer x">Accélération selon x</string>
            <string original="Accelerometer y">Accélération selon y</string>
            <string original="Accelerometer z">Accélération selon z</string>
            <string original="Absolute acceleration">Norme du vecteur accélération</string>
            <string original="time">temps</string>
            <string original="Acceleration">Accélération</string>
            <string original="Acceleration x">Accélération selon x</string>
            <string original="Acceleration y">Accélération selon y</string>
            <string original="Acceleration z">Accélération selon z</string>
            <string original="Absolute">Norme</string>
            <string original="Simple">Composantes</string>
            <string original="Raw Data">Raw Data</string>
        </translation>
        <translation locale="de">
            <title>Beschleunigung</title>
            <category>Arduino Nano 33 BLE Sense</category>
            <description>Beschleunigung mit dem LSM9DS1 IMU des Arduino Nano 33 BLE Sense messen.</description>
            <link label="Weitere Informationen (englisch)">http://www.vulgarisation.fr/?lang=en</link>
            <string original="Accelerometer x">Beschleunigung x</string>
            <string original="Accelerometer y">Beschleunigung y</string>
            <string original="Accelerometer z">Beschleunigung z</string>
            <string original="Absolute acceleration">Absolute Beschleunigung</string>
            <string original="time">Zeit</string>
            <string original="Acceleration">Beschleunigung</string>
            <string original="Acceleration x">Beschleunigung x</string>
            <string original="Acceleration y">Beschleunigung y</string>
            <string original="Acceleration z">Beschleunigung z</string>
            <string original="Absolute">Betrag</string>
            <string original="Simple">Einfach</string>
            <string original="Raw Data">Rohdaten</string>
        </translation>
    </translations>
    <data-containers>
	    <container size="0" static="false" init="">CH1</container>
	    <container size="0" static="false" init="">CH2</container>
	    <container size="0" static="false" init="">CH3</container>
	    <container size="0" static="false" init="">CH4</container>
	    <container size="0" static="false" init="">CH5</container>
	    <container size="0" static="false" init="">CH0</container>
	    <!-- container for normalization to m/s² -->
	    <container size="0" static="false" init="">CH2_norm</container>
	    <container size="0" static="false" init="">CH3_norm</container>
	    <container size="0" static="false" init="">CH4_norm</container>
	    <container size="0" static="false" init="">CH5_norm</container>
    </data-containers>
    <input>
        <bluetooth mode="notification" id="Sense" subscribeOnStart="false">
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="0">CH1</output>
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="4">CH2</output>
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="8">CH3</output>
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="12">CH4</output>
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="16">CH5</output>
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" extra="time">CH0</output>
        </bluetooth>
    </input>
    <output>
        <bluetooth id="Sense">
            <config char="cddf1003-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian">1.0</config>
        </bluetooth>
    </output>
    <views>
        <view label="Graph">
            <graph label="Accelerometer x" aspectRatio="2.5" style="lines" lineWidth="1" color="green" partialUpdate="false" history="1" labelX="t" labelY="a" labelZ="" unitX="s" unitY="m/s²" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH2_norm</input>
            </graph>
            <graph label="Accelerometer y" aspectRatio="2.5" style="lines" lineWidth="1" color="blue" partialUpdate="false" history="1" labelX="t" labelY="a" labelZ="" unitX="s" unitY="m/s²" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH3_norm</input>
            </graph>
            <graph label="Accelerometer z" aspectRatio="2.5" style="lines" lineWidth="1" color="yellow" partialUpdate="false" history="1" labelX="t" labelY="a" labelZ="" unitX="s" unitY="m/s²" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH4_norm</input>
            </graph>
        </view>
        
        <view label="Absolute">
            <graph label="Absolute acceleration" aspectRatio="2.5" style="lines" lineWidth="1" color="white" partialUpdate="false" history="1" labelX="t" labelY="a" labelZ="" unitX="s" unitY="m/s²" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH5_norm</input>
            </graph>
            <value label="Absolute acceleration" size="2" precision="3" unit="m/s²" color="white">
                <input>CH5_norm</input>
            </value>
        </view>
        
        <view label="Multi">
            <graph label="Acceleration" aspectRatio="2.5" style="lines" lineWidth="1" partialUpdate="true" history="1" labelX="t" labelY="a" labelZ="" unitX="s" unitY="m/s²" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x" color="green">CH1</input>
                <input axis="y">CH2_norm</input>
                <input axis="x" color="blue">CH1</input>
                <input axis="y">CH3_norm</input>
                <input axis="x" color="yellow">CH1</input>
                <input axis="y">CH4_norm</input>
                <input axis="x" color="white">CH1</input>
                <input axis="y">CH5_norm</input>
            </graph>
            <value label="Accelerometer x" size="2" precision="2" unit="m/s²" color="green">
                <input>CH2_norm</input>
            </value>
            <value label="Accelerometer y" size="2" precision="2" unit="m/s²" color="blue">
                <input>CH3_norm</input>
            </value>
            <value label="Accelerometer z" size="2" precision="2" unit="m/s²" color="yellow">
                <input>CH4_norm</input>
            </value>
            <value label="Absolute acceleration" size="2" precision="2" unit="m/s²" color="white">
                <input>CH5_norm</input>
            </value>
        </view>
        
        <view label="Simple">
            <value label="Accelerometer x" size="2" precision="3" unit="m/s²" color="green">
                <input>CH2_norm</input>
            </value>
            <value label="Accelerometer y" size="2" precision="3" unit="m/s²" color="blue">
                <input>CH3_norm</input>
            </value>
            <value label="Accelerometer z" size="2" precision="3" unit="m/s²" color="yellow">
                <input>CH4_norm</input>
            </value>
            <value label="Absolute acceleration" size="2" precision="3" unit="m/s²" color="white">
                <input>CH5_norm</input>
            </value>
        </view>
        
        <!-- original view of raw sensor data: multiples of g -->
        <view label="Raw Data">
            <graph label="Accelerometer x" aspectRatio="2.5" style="lines" lineWidth="1" color="ff7e22" partialUpdate="false" history="1" labelX="t" labelY="a" labelZ="" unitX="s" unitY="g" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH2</input>
            </graph>
            <graph label="Accelerometer y" aspectRatio="2.5" style="lines" lineWidth="1" color="ff7e22" partialUpdate="false" history="1" labelX="t" labelY="a" labelZ="" unitX="s" unitY="g" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH3</input>
            </graph>
            <graph label="Accelerometer z" aspectRatio="2.5" style="lines" lineWidth="1" color="ff7e22" partialUpdate="false" history="1" labelX="t" labelY="a" labelZ="" unitX="s" unitY="g" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH4</input>
            </graph>
            <graph label="Absolute acceleration" aspectRatio="2.5" style="lines" lineWidth="1" color="ff7e22" partialUpdate="false" history="1" labelX="t" labelY="a" labelZ="" unitX="s" unitY="g" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH5</input>
            </graph>
        </view>
    </views>
    
    <!-- multiples of g to m/s² -->
    <analysis sleep="0" onUserInput="false">
        <formula formula="[1_]*9.81">
            <input clear="false">CH2</input>
            <output>CH2_norm</output>
        </formula>
        <formula formula="[1_]*9.81">
            <input clear="false">CH3</input>
            <output>CH3_norm</output>
        </formula>
        <formula formula="[1_]*9.81">
            <input clear="false">CH4</input>
            <output>CH4_norm</output>
        </formula>
        <formula formula="[1_]*9.81">
            <input clear="false">CH5</input>
            <output>CH5_norm</output>
        </formula>
    </analysis>
    <export>
        <set name="Raw data (m/s²)">
            <data name="Time t (s)">CH1</data>
            <data name="Acceleration x (m/s²)">CH2_norm</data>
            <data name="Acceleration y (m/s²)">CH3_norm</data>
            <data name="Acceleration z (m/s²)">CH4_norm</data>
            <data name="Absolute Acceleration (m/s²)">CH5_norm</data>
        </set>
        <set name="Raw data (g)">
            <data name="Time t (s)">CH1</data>
            <data name="Acceleration x (g)">CH2</data>
            <data name="Acceleration y (g)">CH3</data>
            <data name="Acceleration z (g)">CH4</data>
            <data name="Absolute Acceleration (g)">CH5</data>
        </set>
    </export>
</phyphox>
