<phyphox version="1.7" locale="en">    
    <title>Angular Velocity (Gyroscope)</title>
    <category>Arduino Nano 33 BLE Sense</category>
    <description>Plots angular velocity (Gyroscope) measured by the Arduino Nano 33 BLE Sense's LSM9DS1 IMU</description>
    <link label="Find more">www.physicsreimagined.com</link>
<icon format="base64"></icon> 
	<color>009297</color> 
    <translations>
        <translation locale="fr">
            <title>Gyroscope (Vitesse angulaire)</title>
            <category>Arduino Nano 33 BLE Sense</category>
            <description>Trace la vitesse angulaire mesurée par les capteurs de la carte Arduino.</description>
            <link label="Plus d'infos">http://www.vulgarisation.fr</link>
            <string original="Gyroscope x">vitesse angulaire autour de l'axe x</string>
            <string original="Gyroscope y">vitesse angulaire autour de l'axe y</string>
            <string original="Gyroscope z">vitesse angulaire autour de l'axe z</string>
            <string original="Absolute">Norme</string>
            <string original="time">temps</string>
            <string original="X angular velocity">vitesse angulaire autour de l'axe x</string>
            <string original="Y angular velocity">vitesse angulaire autour de l'axe y</string>
            <string original="Z angular velocity">vitesse angulaire autour de l'axe z</string>
            <string original="Absolute angular velocity">norme de la vitesse angulaire</string>
            <string original="Angular velocity">vitesse angulaire</string>
            <string original="Simple">Composantes</string>
            <string original="Raw Data">Raw Data</string>
        </translation>
        <translation locale="de">
            <title>Drehrate (Gyroskop)</title>
            <category>Arduino Nano 33 BLE Sense</category>
            <description>Drehrate (Gyroskop) mit dem LSM9DS1 IMU des Arduino Nano 33 BLE Sense messen.</description>
            <link label="Weitere Informationen (englisch)">http://www.vulgarisation.fr/?lang=en</link>
            <string original="Gyroscope x">Gyroskop x</string>
            <string original="Gyroscope y">Gyroskop y</string>
            <string original="Gyroscope z">Gyroskop z</string>
            <string original="Absolute">Betrag</string>
            <string original="time">Zeit</string>
            <string original="X angular velocity">Drehrate x</string>
            <string original="Y angular velocity">Drehrate y</string>
            <string original="Z angular velocity">Drehrate z</string>
            <string original="Absolute angular velocity">Betrag der Drehrate</string>
            <string original="Angular velocity">Drehrate</string>
            <string original="Simple">Einfach</string>
            <string original="Raw Data">Rohdaten</string>
        </translation>
    </translations>
    <data-containers>
	    <container size="0" static="false" init="">CH1</container>
	    <container size="0" static="false" init="">CH2</container>
	    <container size="0" static="false" init="">CH3</container>
	    <container size="0" static="false" init="">CH4</container>
	    <container size="0" static="false" init="">CH5</container>
	    <container size="0" static="false" init="">CH0</container>
	    <!-- container for normalization to rad/s -->
	    <container size="0" static="false" init="">CH2_norm</container>
	    <container size="0" static="false" init="">CH3_norm</container>
	    <container size="0" static="false" init="">CH4_norm</container>
	    <container size="0" static="false" init="">CH5_norm</container>
    </data-containers>
    <input>
        <bluetooth mode="notification" rate="1" subscribeOnStart="false" id="Sense">
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="0">CH1</output>
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="4">CH2</output>
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="8">CH3</output>
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="12">CH4</output>
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="16">CH5</output>
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" extra="time">CH0</output>
        </bluetooth>
    </input>
    <output>
        <bluetooth id="Sense">
            <config char="cddf1003-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian">2.0</config>
        </bluetooth>
    </output>
    <views>
        
        <view label="Graph">
            <graph label="Gyroscope x" aspectRatio="2.5" style="lines" lineWidth="1" color="green" partialUpdate="false" history="1" labelX="t" labelY="ω" labelZ="" unitX="s" unitY="rad/s" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH2_norm</input>
            </graph>
            <graph label="Gyroscope y" aspectRatio="2.5" style="lines" lineWidth="1" color="blue" partialUpdate="false" history="1" labelX="t" labelY="ω" labelZ="" unitX="s" unitY="rad/s" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH3_norm</input>
            </graph>
            <graph label="Gyroscope z" aspectRatio="2.5" style="lines" lineWidth="1" color="yellow" partialUpdate="false" history="1" labelX="t" labelY="ω" labelZ="" unitX="s" unitY="rad/s" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH4_norm</input>
            </graph>
        </view>
        
        <view label="Absolute">
            <graph label="Absolute angular velocity" aspectRatio="2.5" style="lines" lineWidth="1" color="white" partialUpdate="false" history="1" labelX="t" labelY="ω" labelZ="" unitX="s" unitY="rad/s" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH5_norm</input>
            </graph>
            <value label="Absolute angular velocity" size="2" precision="3" unit="rad/s" color="white">
                <input>CH5_norm</input>
            </value>
        </view>
        
        <view label="Multi">
            <graph label="Angular velocity" aspectRatio="2.5" style="lines" lineWidth="1" partialUpdate="true" history="1" labelX="t" labelY="ω" labelZ="" unitX="s" unitY="rad/s" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x" color="green">CH1</input>
                <input axis="y">CH2_norm</input>
                <input axis="x" color="blue">CH1</input>
                <input axis="y">CH3_norm</input>
                <input axis="x" color="yellow">CH1</input>
                <input axis="y">CH4_norm</input>
                <input axis="x" color="white">CH1</input>
                <input axis="y">CH5_norm</input>
            </graph>
            <value label="Gyroscope x" size="2" precision="2" unit="rad/s" color="green">
                <input>CH2_norm</input>
            </value>
            <value label="Gyroscope y" size="2" precision="2" unit="rad/s" color="blue">
                <input>CH3_norm</input>
            </value>
            <value label="Gyroscope z" size="2" precision="2" unit="rad/s" color="yellow">
                <input>CH4_norm</input>
            </value>
            <value label="Absolute" size="2" precision="2" unit="rad/s" color="white">
                <input>CH5_norm</input>
            </value>
        </view>
        
        <view label="Simple">
            <value label="Gyroscope x" size="2" precision="3" unit="rad/s" color="green">
                <input>CH2_norm</input>
            </value>
            <value label="Gyroscope y" size="2" precision="3" unit="rad/s" color="blue">
                <input>CH3_norm</input>
            </value>
            <value label="Gyroscope z" size="2" precision="3" unit="rad/s" color="yellow">
                <input>CH4_norm</input>
            </value>
            <value label="Absolute" size="2" precision="3" unit="rad/s" color="white">
                <input>CH5_norm</input>
            </value>
        </view>
        
        <!-- original view of raw sensor data: deg/s -->
        <view label="Raw Data">
            <graph label="Gyroscope x" aspectRatio="2.5" style="lines" lineWidth="1" color="ff7e22" partialUpdate="false" history="1" labelX="t" labelY="ω" labelZ="" unitX="s" unitY="deg/s" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH2</input>
            </graph>
            <graph label="Gyroscope y" aspectRatio="2.5" style="lines" lineWidth="1" color="ff7e22" partialUpdate="false" history="1" labelX="t" labelY="ω" labelZ="" unitX="s" unitY="deg/s" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH3</input>
            </graph>
            <graph label="Gyroscope z" aspectRatio="2.5" style="lines" lineWidth="1" color="ff7e22" partialUpdate="false" history="1" labelX="t" labelY="ω" labelZ="" unitX="s" unitY="deg/s" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH4</input>
            </graph>
            <graph label="Absolute" aspectRatio="2.5" style="lines" lineWidth="1" color="ff7e22" partialUpdate="false" history="1" labelX="t" labelY="ω" labelZ="" unitX="s" unitY="deg/s" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH5</input>
            </graph>
        </view>
    </views>
    
    <!-- deg/s to rad/s -->
    <analysis sleep="0" onUserInput="false">
        <formula formula="[1_]*3.14159/180">
            <input clear="false">CH2</input>
            <output>CH2_norm</output>
        </formula>
        <formula formula="[1_]*3.14159/180">
            <input clear="false">CH3</input>
            <output>CH3_norm</output>
        </formula>
        <formula formula="[1_]*3.14159/180">
            <input clear="false">CH4</input>
            <output>CH4_norm</output>
        </formula>
        <formula formula="[1_]*3.14159/180">
            <input clear="false">CH5</input>
            <output>CH5_norm</output>
        </formula>
    </analysis>
    <export>
    <set name="Raw data (rad/s)">
            <data name="Time t (s)">CH1</data>
            <data name="Angular Velocity x (rad/s)">CH2_norm</data>
            <data name="Angular Velocity y (rad/s)">CH3_norm</data>
            <data name="Angular Velocity z (rad/s)">CH4_norm</data>
            <data name="Absolute Angular Velocity (rad/s)">CH5_norm</data>
        </set>
        <set name="Raw data (deg/s)">
            <data name="Time t (s)">CH1</data>
            <data name="Angular Velocity x (deg/s)">CH2</data>
            <data name="Angular Velocity y (deg/s)">CH3</data>
            <data name="Angular Velocity z (deg/s)">CH4</data>
            <data name="Absolute Angular Velocity (deg/s)">CH5</data>
        </set>
    </export>
</phyphox>
