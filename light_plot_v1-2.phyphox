            <value label="Blue" size="2" precision="2" unit="a.u." color="blue">
                <input>CH5</input>
            </value>
        </view>
        
    </views>
    <analysis sleep="0" onUserInput="false"/>
    <export>
        <set name="Raw data">
            <data name="Time t (s)">CH1</data>
            <data name="Illuminance (a.u.)">CH2</data>
            <data name="Red (a.u.)">CH3</data>
            <data name="Green (a.u.)">CH4</data>
            <data name="Blue (a.u.)">CH5</data>
        </set>
    </export>
</phyphox>