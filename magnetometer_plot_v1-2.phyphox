<phyphox version="1.7" locale="en">
    <title>Magnetic Field</title>
    <category>Arduino Nano 33 BLE Sense</category>
    <description>Plots magnetic field measured by the Arduino Nano 33 BLE Sense's LSM9DS1 IMU</description>
    <link label="Find more">www.physicsreimagined.com</link>
  <icon format="base64"></icon>
	<color>009297</color> 
    <translations>
        <translation locale="fr">
            <title>Champ magnétique</title>
            <category>Arduino Nano 33 BLE Sense</category>
            <description>Trace le champ magnétique mesuré par les capteurs de la carte Arduino.</description>
            <link label="Plus d'infos">http://www.vulgarisation.fr</link>
            <string original="Magnetometer x">champ magnétique selon x</string>
            <string original="Magnetometer y">champ magnétique selon y</string>
            <string original="Magnetometer z">champ magnétique selon z</string>
            <string original="Magnetometer">champ magnétique</string>
            <string original="Absolute magnetic field">Norme du champ magnétique</string>
            <string original="time">temps</string>
            <string original="Magnetic field">Champ magnétique</string>
            <string original="Absolute">Norme</string>
            <string original="Simple">Composantes</string>
        </translation>
        <translation locale="de">
            <title>Magnetfeld</title>
            <category>Arduino Nano 33 BLE Sense</category>
            <description>Magnetfeld mit dem LSM9DS1 IMU des Arduino Nano 33 BLE Sense messen.</description>
            <link label="Weitere Informationen (englisch)">http://www.vulgarisation.fr/?lang=en</link>
            <string original="Magnetometer x">Magnetometer x</string>
            <string original="Magnetometer y">Magnetometer y</string>
            <string original="Magnetometer z">Magnetometer z</string>
            <string original="Magnetometer">Magnetometer</string>
            <string original="Absolute magnetic field">Betrag des Magnetfelds</string>
            <string original="time">Zeit</string>
            <string original="Magnetic field">Magnetfeld</string>
            <string original="Absolute">Betrag</string>
            <string original="Simple">Einfach</string>
        </translation>
    </translations>
    <data-containers>
	    <container size="0" static="false" init="">CH1</container>
	    <container size="0" static="false" init="">CH2</container>
	    <container size="0" static="false" init="">CH3</container>
	    <container size="0" static="false" init="">CH4</container>
	    <container size="0" static="false" init="">CH5</container>
	    <container size="0" static="false" init="">CH0</container>
    </data-containers>
    <input>
        <bluetooth mode="notification" rate="1" subscribeOnStart="false" id="Sense">
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="0">CH1</output>
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="4">CH2</output>
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="8">CH3</output>
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="12">CH4</output>
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="16">CH5</output>
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" extra="time">CH0</output>
        </bluetooth>
    </input>
    <output>
        <bluetooth id="Sense">
            <config char="cddf1003-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian">3.0</config>
        </bluetooth>
    </output>
    <views>
        <view label="Graph">
            <graph label="Magnetometer x" aspectRatio="2.5" style="lines" lineWidth="1" color="green" partialUpdate="false" history="1" labelX="time" labelY="B" labelZ="" unitX="s" unitY="µT" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH2</input>
            </graph>
            <graph label="Magnetometer y" aspectRatio="2.5" style="lines" lineWidth="1" color="blue" partialUpdate="false" history="1" labelX="time" labelY="B" labelZ="" unitX="s" unitY="µT" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH3</input>
            </graph>
            <graph label="Magnetometer z" aspectRatio="2.5" style="lines" lineWidth="1" color="yellow" partialUpdate="false" history="1" labelX="time" labelY="B" labelZ="" unitX="s" unitY="µT" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH4</input>
            </graph>
        </view>
        
        <view label="Absolute">
            <graph label="Absolute magnetic field" aspectRatio="2.5" style="lines" lineWidth="1" color="white" partialUpdate="false" history="1" labelX="time" labelY="B" labelZ="" unitX="s" unitY="µT" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x">CH1</input>
                <input axis="y">CH5</input>
            </graph>
            <value label="Absolute magnetic field" size="2" precision="3" unit="µT" color="white">
                <input>CH5</input>
            </value>
        </view>
        
        <view label="Multi">
            <graph label="Magnetic field" aspectRatio="2.5" style="lines" lineWidth="1" partialUpdate="true" history="1" labelX="t" labelY="B" labelZ="" unitX="s" unitY="µT" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0" mapWidth="0">
                <input axis="x" color="green">CH1</input>
                <input axis="y">CH2</input>
                <input axis="x" color="blue">CH1</input>
                <input axis="y">CH3</input>
                <input axis="x" color="yellow">CH1</input>
                <input axis="y">CH4</input>
                <input axis="x" color="white">CH1</input>
                <input axis="y">CH5</input>
            </graph>
            <value label="Magnetometer x" size="2" precision="2" unit="µT" color="green">
                <input>CH2</input>
            </value>
            <value label="Magnetometer y" size="2" precision="2" unit="µT" color="blue">
                <input>CH3</input>
            </value>
            <value label="Magnetometer z" size="2" precision="2" unit="µT" color="yellow">
                <input>CH4</input>
            </value>
            <value label="Absolute magnetic field" size="2" precision="2" unit="µT" color="white">
                <input>CH5</input>
            </value>
        </view>
        
         <view label="Simple">
            <value label="Magnetometer x" size="2" precision="2" unit="µT" color="green">
                <input>CH2</input>
            </value>
            <value label="Magnetometer y" size="2" precision="2" unit="µT" color="blue">
                <input>CH3</input>
            </value>
            <value label="Magnetometer z" size="2" precision="2" unit="µT" color="yellow">
                <input>CH4</input>
            </value>
            <value label="Absolute magnetic field" size="2" precision="2" unit="µT" color="white">
                <input>CH5</input>
            </value>
        </view>
    </views>
    <analysis sleep="0" onUserInput="false"/>
    <export>
        <set name="Raw data (µT)">
            <data name="Time t (s)">CH1</data>
            <data name="Magnetometer x (µT)">CH2</data>
            <data name="Magnetometer y (µT)">CH3</data>
            <data name="Magnetometer z (µT)">CH4</data>
            <data name="Absolute magnetic field (µT)">CH5</data>
        </set>
    </export>
</phyphox>
